    skos:notation "4.2.2" ;
    skos:prefLabel "einfache Geschichten erzählen."@de .

<https://w3id.org/iqb/bista23FrESA/funktionalekommunikativekompetenz/f698aee6-87fc-4f62-ba0e-c80770dde044> a skos:Concept ;
    skos:broader <https://w3id.org/iqb/bista23FrESA/funktionalekommunikativekompetenz/a1afbafa-ed6f-4149-8b15-50480ad0c609> ;
    skos:inScheme <https://w3id.org/iqb/bista23FrESA/funktionalekommunikativekompetenz/> ;
    skos:notation "3.1.3" ;
